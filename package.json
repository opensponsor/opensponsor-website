{
  "name": "opensponsor-website",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:en": "ng serve --configuration=development-en --open",
    "extract": "ng extract-i18n --format=xlf --output-path=src/locale",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:opensponsor-website": "node dist/opensponsor-website/server/server.mjs",
    "generateInterface": "pnpx swagger-typescript-api generate -p http://localhost:8080/q/openapi --no-client -o ./src/app/interfaces --name ApiInterface.ts",
    "gi": "pnpm run generateInterface && git commit ./src/app/interfaces/ApiInterface.ts -m 'update ApiInterface.ts'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.3",
    "@angular/cdk": "^20.0.3",
    "@angular/common": "^20.0.3",
    "@angular/compiler": "^20.0.3",
    "@angular/core": "^20.0.3",
    "@angular/forms": "^20.0.3",
    "@angular/material": "^20.0.3",
    "@angular/platform-browser": "^20.0.3",
    "@angular/platform-browser-dynamic": "^20.0.3",
    "@angular/platform-server": "^20.0.3",
    "@angular/router": "^20.0.3",
    "@angular/ssr": "^19.2.4",
    "@octokit/oauth-methods": "^5.1.3",
    "express": "^4.18.2",
    "limax": "^4.1.0",
    "ng-extract-i18n-merge": "^2.15.1",
    "qrcanvas": "^3.1.2",
    "rxjs": "~7.8.0",
    "swiper": "^11.1.15",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.4",
    "@angular/cli": "20.0.2",
    "@angular/compiler-cli": "^20.0.3",
    "@angular/localize": "20.0.3",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.3"
  }
}
